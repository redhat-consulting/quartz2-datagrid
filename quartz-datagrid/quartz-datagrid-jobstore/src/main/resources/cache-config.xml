<infinispan
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:infinispan:config:7.1 http://www.infinispan.org/schemas/infinispan-config-7.1.xsd"
        xmlns="urn:infinispan:config:7.1">

   <cache-container name="defaultCacheManager" default-cache="default">
       <local-cache name="default" />
       <local-cache name="triggersByKey" />
       <local-cache name="triggersByGroup" />
       <local-cache name="timeTriggers" />
       <local-cache name="triggers" />
       <local-cache name="pausedTriggerGroups" />
       <local-cache name="jobsByKey" />
       <local-cache name="jobsByGroup" />
       <local-cache name="pausedGroups" />
       <local-cache name="blockedJobs" />
       <local-cache name="calendars" />
   </cache-container>

</infinispan>

